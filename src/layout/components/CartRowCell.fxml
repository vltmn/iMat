<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="cart-row-cell" stylesheets="@../styles.css" type="HBox" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ImageView fx:id="productImage" fitHeight="50.0" fitWidth="50.0" />
        <VBox fx:id="nameAndQtyPane" HBox.hgrow="ALWAYS">
            <children>
                <Label fx:id="productName" text="potatis" />

            </children>
        </VBox>
      <VBox alignment="BOTTOM_RIGHT">
         <children>
            <Label fx:id="totalLabel" text="100:0" />
         </children>
      </VBox>
    </children>

</fx:root>
