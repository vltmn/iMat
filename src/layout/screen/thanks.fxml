<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="CENTER" fillWidth="false" styleClass="modal-wrapper" stylesheets="@../styles.css" type="VBox" xmlns="http://javafx.com/javafx/9.0.4" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox alignment="CENTER" prefHeight="350.0" prefWidth="600.0" spacing="50.0">
            <children>
                <Label text="Tack för din beställning!">
                    <font>
                        <Font size="45.0" />
                    </font>
                </Label>
            <Button mnemonicParsing="false" onAction="#onProceedClick" scaleX="1.5" scaleY="1.5" text="Fortsätt handla">
               <font>
                  <Font size="24.0" />
               </font>
               <styleClass>
                  <String fx:value="btn" />
                  <String fx:value="btn-default" />
               </styleClass>
            </Button>
                <Button mnemonicParsing="false" onAction="#onTerminateClick" text="Avsluta programmet">
                    <font>
                        <Font size="24.0" />
                    </font>
               <styleClass>
                  <String fx:value="btn" />
                  <String fx:value="btn-warning-outline" />
               </styleClass>
                </Button>
            </children>
         <styleClass>
            <String fx:value="bg" />
            <String fx:value="thanks-screen" />
         </styleClass>
        </VBox>
    </children>
</fx:root>
