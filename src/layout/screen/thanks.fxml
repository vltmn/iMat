    <children>
        <VBox alignment="CENTER" prefHeight="350.0" prefWidth="600.0" style="-fx-background-color: -fx-background;">
            <children>
                <Label text="Tack för din beställning">
                    <font>
                        <Font size="45.0" />
                    </font>
                </Label>
                <Button mnemonicParsing="false" onAction="#onClick" prefHeight="65.0" prefWidth="117.0" styleClass="btn, btn-big" text="Stäng">
                    <font>
                        <Font size="24.0" />
                    </font>
                </Button>
            </children>
        </VBox>
    </children>
</fx:root>
